{% extends 'partials/base.html.twig' %}
{% block content %}
  <article class="post">
    <header>
      <div class="title">
        <h2>{{ form.message }}</h2>
      </div>
      <div class="meta">
        <time class="published" datetime="{{form.date|td(null, grav.config.system.pages.dateformat.short)}}">{{ form.date|td(null, grav.config.system.pages.dateformat.short)}}</time>
      </div>
    </header>
    <p>{{'FUTURE.FORM_SUMMARY'|t}}</p>

      {{ content|raw }}

      {% include "forms/data.html.twig" %}
  </article>
{% endblock %}
